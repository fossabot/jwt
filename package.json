{
  "name": "@tadashi/jwt",
  "version": "1.0.6",
  "description": "Generates, verifies and parses a JSON Web Token",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "doc": "jsdoc index.js -t node_modules/docdash -d ./doc/",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "keywords": [
    "jwt",
    "sign",
    "verify",
    "parse"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "dependencies": {
    "@tadashi/debug": "2.0.0",
    "jsrsasign": "8.0.4",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "coveralls": "3.0.0",
    "docdash": "0.4.0",
    "jsdoc": "3.5.5",
    "nyc": "11.4.1",
    "xo": "0.18.2"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  }
}
