{
  "name": "@tadashi/jwt",
  "version": "2.0.0",
  "description": "Sign, verify and parse a JSON Web Token",
  "main": "index.js",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "NODE_ENV=test ava",
    "test-cov": "NODE_ENV=test nyc ava"
  },
  "keywords": [
    "jwt",
    "sign",
    "verify",
    "parse"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.1"
  },
  "dependencies": {
    "@tadashi/debug": "2.1.0",
    "jsrsasign": "8.0.8",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "nyc": "11.6.0",
    "xo": "0.20.3"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  }
}
